{
  "name": "s4e-autos",
  "version": "1.0.0",
  "description": "prototype - a simple GraphQL demo rendered with Svelte",
  "author": "eswat2",
  "license": "MIT",
  "repository": "eswat2/s4e-autos",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "echo skipping...",
    "predeploy": "yarn format; yarn build",
    "deploy": "cd dist; ln -s ../.now .vercel; vc --prod",
    "cc": "prettier --check './src/**/*.{css,js,ts,svelte}'",
    "format": "prettier --write './src/**/*.{css,js,ts,svelte}'",
    "spell": "cspell './src/**/*.{css,js,ts,svelte}'",
    "open": "open https://s4e-autos.vercel.app"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "2.4.3",
    "graphql": "16.7.1",
    "prettier": "3.0.0",
    "prettier-plugin-svelte": "3.0.0",
    "smoo": "0.0.17",
    "svelte": "4.1.1",
    "sveltestrap": "5.10.0",
    "svql": "0.0.35",
    "vite": "4.4.6"
  }
}
